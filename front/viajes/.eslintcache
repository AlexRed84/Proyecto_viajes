{register()} />\n              <input className=\"botonLogin\" type=\"submit\" value=\"Actualizar Información\" />\n              <hr></hr>\n              <h4 className=\"registrado\" onClick={onDelete}>\n                Borrar cuenta\n              </h4>\n              {statusMessage.length > 0 && <p className=\"status-ok\">{statusMessage}</p>}\n              {errorMessage.length > 0 && <p className=\"error\">{errorMessage}</p>}\n            </form>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n}\n","/home/ale/Escritorio/App_Viajes/Proyecto_viajando/front/viajes/src/components/security/NotUserLogged.js",[],"/home/ale/Escritorio/App_Viajes/Proyecto_viajando/front/viajes/src/components/security/LogedUser.js",[],"/home/ale/Escritorio/App_Viajes/Proyecto_viajando/front/viajes/src/shared/utils/decodeTokenData.js",[],"/home/ale/Escritorio/App_Viajes/Proyecto_viajando/front/viajes/src/http/api2.js",[],"/home/ale/Escritorio/App_Viajes/Proyecto_viajando/front/viajes/src/components/components/BackgroundVideo.js",[],{"ruleId":"127","replacedBy":"128"},{"ruleId":"129","replacedBy":"130"},{"ruleId":"131","severity":1,"message":"132","line":19,"column":6,"nodeType":"133","endLine":19,"endColumn":16,"suggestions":"134"},"no-native-reassign",["135"],"no-negated-in-lhs",["136"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'profileData?.data?.photo?.data'. Either include it or remove the dependency array.","ArrayExpression",["137"],"no-global-assign","no-unsafe-negation",{"desc":"138","fix":"139"},"Update the dependencies array to be: [profileData?.data?.photo?.data, userData]",{"range":"140","text":"141"},[794,804],"[profileData?.data?.photo?.data, userData]"]